{
  "name": "playwright-cucumber",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "cucumber-js -r ts-node/register -r tsconfig-paths/register",
    "test:docker": "docker-compose up --build --abort-on-container-exit",
    "test:docker:rebuild": "docker-compose up --build --force-recreate --abort-on-container-exit",
    "test:headed": "HEADLESS=false npm test",
    "docker:build": "docker build -t playwright-cucumber .",
    "docker:run": "docker run --rm -v ./test-results:/app/test-results playwright-cucumber",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "vault:init": "VAULT_ADDR=http://localhost:8200 VAULT_TOKEN=dev-only-token UI_MODE=true npx ts-node scripts/init-vault.ts",
    "vault:start": "docker-compose up vault -d",
    "vault:stop": "docker-compose stop vault",
    "vault:diagnose": "bash scripts/diagnose-vault.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@cucumber/cucumber": "^11.2.0",
    "@playwright/test": "^1.52.0",
    "@types/node": "^22.15.17",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "dotenv": "^16.5.0"
  }
}
